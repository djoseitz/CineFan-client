"components/login-view/login-view.jsx","original":{"line":29,"column":2}},{"generated":{"line":88,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":55,"column":1}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from 'react';\nimport Form from 'react-bootstrap/Form';\nimport Button from 'react-bootstrap/Button';\nimport axios from 'axios';\nimport { Link } from 'react-router-dom';\nimport { Container } from 'react-bootstrap';\n\nexport function LoginView(props) {\n  const [username, setUsername] = useState('');\n  const [password, setPassword] = useState('');\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    /* Send a request to the server for authentication */\n    axios.post('https://cinefandb.herokuapp.com/login', {\n      Username: username,\n      Password: password\n    })\n    .then(response => {\n      const data = response.data;\n      props.onLoggedIn(data);\n    })\n    .catch(e => {\n      alert('no such user');\n      console.log('no such user')\n    });\n  };\n\n  return (\n    <Container>\n      <Form>\n        <Form.Group controlId=\"formBasicUsername\">\n          <Form.Label>Username:</Form.Label>\n          <Form.Control type=\"text\" placeholder=\"Enter username\" value={username} onChange={e => setUsername(e.target.value)} />\n        </Form.Group>\n\n        <Form.Group controlId=\"formBasicPassword\">\n          <Form.Label>Password</Form.Label>\n          <Form.Control type=\"password\" placeholder=\"Password\" value={password} onChange={e => setPassword(e.target.value)} />\n        </Form.Group>\n        <Button variant=\"primary\" type=\"submit\" onClick={handleSubmit}>\n          Submit\n          </Button>\n          {/* <Link to={`/register`}>\n            <Button\n              className=\"sign-up-button new-user\"\n              variant=\"dark\"\n            >\n            New User Sign Up\n            </Button>\n          </Link> */}\n      </Form>\n    </Container>\n  )\n}"},"lineCount":null}},"error":null,"hash":"833479914870f463e116f3b8ec7f26ba","cacheData":{"env":{}}}